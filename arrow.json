{
    "title": "My style like arrow key right_command",
    "rules": [
    {
        "description": "right_command + w/a/s/d to arrows",
        "manipulators": [
        {
            "from":
            {
                "key_code": "w",
                "modifiers":
                {
                    "mandatory": [
                        "right_command"
                    ],
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [
            {
                "key_code": "up_arrow"
            }],
            "type": "basic"
        },
        {
            "from":
            {
                "key_code": "a",
                "modifiers":
                {
                    "mandatory": [
                        "right_command"
                    ],
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [
            {
                "key_code": "left_arrow"
            }],
            "type": "basic"
        },
        {
            "from":
            {
                "key_code": "s",
                "modifiers":
                {
                    "mandatory": [
                        "right_command"
                    ],
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [
            {
                "key_code": "down_arrow"
            }],
            "type": "basic"
        },
        {
            "from":
            {
                "key_code": "d",
                "modifiers":
                {
                    "mandatory": [
                        "right_command"
                    ],
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [
            {
                "key_code": "right_arrow"
            }],
            "type": "basic"
        }]
    }]
}